            <input type="text" class="form-control search-input" id="traffic_person_first_name"
              name="traffic_person_first_name" placeholder="First Name" data-category="person">
          </div>
          <div class="form-group col-md-2 col-sm-2 col-xs-12">
            <label for="traffic_person_last_name">Last Name</label>
            <input type="text" class="form-control search-input" id="traffic_person_last_name"
              name="traffic_person_last_name" placeholder="Last Name" data-category="person">
          </div>
          <div class="form-group col-md-2 col-sm-2 col-xs-12">
            <label class="col-sm-12" style="margin-left:-10px">Age</label>
            <div class="flex-container">
              <input type="number" class="form-control search-input" id="age_min" name="age_min"
                placeholder="Min" data-category="person" />
              -
              <input type="number" class="form-control search-input" id="age_max" name="age_max"
                placeholder="Max" data-category="person" />

            </div>
          </div>
          <div class="form-group col-md-12 search-section">Place</div>
          <div class="form-group col-md-2 col-sm-2 col-xs-12">
            <label for="crash_location">Address</label>
            <input type="text" class="form-control search-input" id="crash_location" name="crash_location"
